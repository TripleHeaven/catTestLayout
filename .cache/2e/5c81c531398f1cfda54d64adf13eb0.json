een and (min-width: 2000px) {\n\t.product-wrapper {\n\t\twidth: 14%;\n\t}\n}\n/*  */\n@media only screen and (min-width: 450px) {\n\t.block-list:checked ~ .adaptiveList .product-wrapper {\n    width: 95%;\n    min-width: 95%;\n\t}\n}\n\n@media only screen and (min-width: 768px) {\n\t.block-list:checked ~ .adaptiveList .product-wrapper {\n\t\twidth: 75%;\n\t}\n}\n\n@media only screen and (min-width: 1000px) {\n\t.block-list:checked ~ .adaptiveList .product-wrapper {\n\t\twidth: 80%;\n\t}\n}\n@media only screen and (min-width: 2000px) {\n\t.block-list:checked ~ .adaptiveList .product-wrapper {\n\t\twidth: 100%;\n\t}\n}\n.products {\n  margin-right: 30px;\n}\n.product {\n\tdisplay: block;\n\tborder: 1px solid #b5e9a7;\n\tborder-radius: 3px;\n\tposition: relative;\n\tbackground: #fff;\n  margin-right: 10px;\n  margin-bottom: 10px;\n\ttext-decoration: none;\n\tcolor: #474747;\n\tz-index: 0;\n  min-height: 310px;\n  \n  \n}\n.product-photo {\n\tposition: relative;\n\tpadding-bottom: 100%;\n\toverflow: hidden;\n}\n/* Структура не меняется - поэтому оптимальное решение - использование img */\n.product-photo img {\n\tposition: absolute;\n\ttop: 0;\n\tbottom: 0;\n\tleft: 0;\n\tright: 0;\n\tmax-width:95%;\n\tmax-height: 100%;\n\tmargin: auto;\n  transition: transform .8s ease-out;\n  transition: max-width .8s ease-out;\n}\n.product-photo:hover img {\n  box-shadow: 0 0 5px 2px red;\n}\n.product p {\n\tposition: relative;\n\tmargin: 0;\n  line-height: 1.4em;\n  height: 1.5em;\n  overflow: hidden;\n  padding-left: 4px;\n}\n.catName {\n  font-size: 2em;\n  text-overflow: ellipsis;\n}\n"},"lineCount":null}},"error":null,"hash":"88989e81d601641030790a4db5ffb211","cacheData":{"env":{}}}